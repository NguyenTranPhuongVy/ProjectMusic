@{
    var a = "@";
}
<script src="~/Scripts/jquery-3.5.1.min.js"></script>
<link href="~/Content/Semantic-UI-CSS-master/semantic.css" rel="stylesheet" />
<script src="~/Content/Semantic-UI-CSS-master/semantic.js"></script>
<script>
    $('.ui.form')
        .form({
            inline: true,
            on: 'blur',
            fields: {
                user_name: {
                    identifier: 'user_name',
                    rules: [
                        {
                            type: 'empty',
                            prompt: 'Họ và tên không cho phép để trống.'
                        },
                        {
                            type: 'minLength[5]',
                            prompt: 'Không được phép nhập dưới 5 kí tự.'
                        }
                    ]
                },
                user_email: {
                    identifier: 'user_email',
                    rules: [
                        {
                            type: 'empty',
                            prompt: 'Email không được phép để trống.'
                        },
                        {
                            type: 'email',
                            prompt: 'Email không hợp lệ.'
                        }
                    ]
                },
                user_pass: {
                    identifier: 'user_pass',
                    rules: [
                        {
                            type: 'empty',
                            prompt: 'Mật khẩu không cho phép để trống.'
                        },
                        {
                            type: 'regExp[^(?=.*[a-z])(?=.*[A-Z])(?=.*[0-9])(?=.*[!@a#\$%\^&\*])(?=.{8,})]',
                            prompt: 'Mật khẩu phải tối thiểu tám ký tự, ít nhất một chữ cái viết hoa, một chữ cái viết thường, một số và một ký tự đặc biệt'
                        }
                    ]
                },
                user_repeat: {
                    identifier: 'user_repeat',
                    rules: [
                        {
                            type: 'empty',
                            prompt: 'Xác nhận không cho phép để trống.'
                        },
                        {
                            type: 'match[user_pass]',
                            prompt: 'Mật khẩu không trùng khớp.'
                        }
                    ]
                },
                user_newpass: {
                    identifier: 'user_newpass',
                    rules: [
                        {
                            type: 'empty',
                            prompt: 'Mật khẩu không cho phép để trống.'
                        },
                        {
                            type: 'regExp[^(?=.*[a-z])(?=.*[A-Z])(?=.*[0-9])(?=.*[!@a#\$%\^&\*])(?=.{8,})]',
                            prompt: 'Mật khẩu phải tối thiểu tám ký tự, ít nhất một chữ cái viết hoa, một chữ cái viết thường, một số và một ký tự đặc biệt'
                        }
                    ]
                },
                user_repeatedit: {
                    identifier: 'user_repeatedit',
                    rules: [
                        {
                            type: 'empty',
                            prompt: 'Xác nhận không cho phép để trống.'
                        },
                        {
                            type: 'match[user_newpass]',
                            prompt: 'Mật khẩu không trùng khớp.'
                        }
                    ]
                },
                user_oldpass: {
                    identifier: 'user_oldpass',
                    rules: [
                        {
                            type: 'empty',
                            prompt: 'Không Được Phép Để Trống.'
                        }
                    ]
                },
                profile_phone: {
                    identifier: 'profile_phone',
                    rules: [
                        {
                            type: 'empty',
                            prompt: 'Không Được Phép Để Trống.'
                        }
                    ]
                },
                profile_lastname: {
                    identifier: 'profile_lastname',
                    rules: [
                        {
                            type: 'empty',
                            prompt: 'Không Được Phép Để Trống.'
                        }
                    ]
                },
                profile_name: {
                    identifier: 'profile_name',
                    rules: [
                        {
                            type: 'empty',
                            prompt: 'Không Được Phép Để Trống.'
                        }
                    ]
                },
                profile_name: {
                    identifier: 'profile_name',
                    rules: [
                        {
                            type: 'empty',
                            prompt: 'Không Được Phép Để Trống.'
                        }
                    ]
                },
                profile_favorite: {
                    identifier: 'profile_favorite',
                    rules: [
                        {
                            type: 'empty',
                            prompt: 'Không Được Phép Để Trống.'
                        }
                    ]
                },
                profile_note: {
                    identifier: 'profile_note',
                    rules: [
                        {
                            type: 'empty',
                            prompt: 'Không Được Phép Để Trống.'
                        }
                    ]
                },
                profile_address: {
                    identifier: 'profile_address',
                    rules: [
                        {
                            type: 'empty',
                            prompt: 'Không Được Phép Để Trống.'
                        }
                    ]
                }





















            }
        })
        ;
</script>
